{% if dnsmasq.hosts is defined %}
{% for host, entry in dnsmasq.hosts.items() %}
{% if entry.ip is defined %}
{{ entry.ip }}	{{ host }}
{% endif %}
{% endfor %}
{% endif %}

{% if dnsmasq.domains is defined %}
{% for domain, ip in dnsmasq.domains.items() %}
{{ ip }}	{{ domain }}
{% endfor %}
{% endif %}
